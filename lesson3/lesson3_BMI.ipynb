   "source": [
    "#計算BMI\n",
    "name=input('請輸入姓名:')\n",
    "bh=eval(input('請輸入身高(公分):'))\n",
    "bw=eval(input('請輸入體重(公斤):'))\n",
    "#BMI=體重(公斤)/身高²(公尺)²\n",
    "bmi=bw/(bh/100)**2\n",
    "print(f\"{name}\")\n",
    "print(f\"身高:{bh}cm,體重:{bw}公斤\")\n",
    "print(f\"BMI:{bmi:.2f}\")"
   ]
